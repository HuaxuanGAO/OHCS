<h1>Doctor Profile</h1>
<a href="/dashboard">Rooms</a>

<ul id="details">
  <li>
    Username:
    <%= @doctor.username %>
  </li>
  <li>
    Email:
    <%= @doctor.email %>
  </li>
  <li>
    Name:
    <%= @doctor.first_name + " " + @doctor.last_name %>
  </li>
  <li>
    Gender:
    <%= @doctor.gender %>
  </li>
  <li>
    Birthday:
    <%= @doctor.birthday.strftime("%B %d, %Y") %>
  </li>
</ul>

„ÄÅ<div class="row">
  <%= link_to 'Edit Info', '/doctors/editinfo', :class => 'btn btn-primary col-2' %>
  <%= link_to 'Change Password or Delete', '/doctors/edit', :class => 'btn btn-danger col-2' %>
  <%= link_to 'Back to doctor list', '/doctors', :class => 'btn btn-primary col-2' %>
</div>
