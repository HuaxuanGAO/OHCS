<div class="container m-auto my-4">
  <% if !@appointments || @appointments.length == 0 %>    
    <%= render "appointment_cta" %>
  <% else %>
    <%= render "appointment_landing", appointments: @appointments %>
  <% end %>
</div>

<div>
  <%= link_to "Back", "/users/#{@current_user.id}/profile", :class => 'btn btn-primary col-2' %>
</div>
